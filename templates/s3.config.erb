type: S3
config:
  bucket: "<%= @s3bucket %>"
  endpoint: "<%= @s3endpoint %>"
  region: ""
  access_key: "<%= @s3access_key %>"
  insecure: true
  signature_version2: false
  encrypt_sse: false
  secret_key: "<%= @s3secret_key %>"
  put_user_metadata: {}
  http_config:
    idle_conn_timeout: 90s
    response_header_timeout: 2m
    insecure_skip_verify: false
  trace:
    enable: false
  part_size: 0